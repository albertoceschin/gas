FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 37.5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1e-5;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    wall
    {
        type            kqRWallFunction;
        value           uniform 1;
    }

    inlet_wall
    {
        type            kqRWallFunction;
        value           uniform 1;
    }
}


// ************************************************************************* //

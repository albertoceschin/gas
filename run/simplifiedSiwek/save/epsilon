FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 5390.5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1e-5;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    wall
    {
        type            epsilonWallFunction;
        value           uniform 5390.5;
    }
    
    wall_inlet
    {
        type            epsilonWallFunction;
        value           uniform 5390.5;
    }
}


// ************************************************************************* //
